<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mapper.AuthorProfileMapper"> 
	<insert id="insertProfile" parameterType="apdto">
		insert into author_profile (history,history_file,idea,idea_file,identity,identity_file,writeday)
		values (#{history},#{history_file},#{idea},#{idea_file},#{identity},#{identity_file},now())
	</insert>
</mapper>